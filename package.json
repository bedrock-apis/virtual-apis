{
   "name": "virtual-apis",
   "description": "",
   "version": "1.0.0",
   "author": "bedrock-apis",
   "license": "Apache-2.0",
   "keywords": [],
   "private": true,
   "type": "module",
   "workspaces": [
      "packages/*"
   ],
   "scripts": {
      " ": "",
      "  ": "",
      "   ": "",
      "    ": "",
      "build": "pnpm run build:src && pnpm run build:images",
      "build:images": "node --enable-source-maps ./packages/va-image-generator",
      "build:src": "rolldown -c rolldown.config.ts",
      "check": "pnpm run build && pnpm run tlt && pnpm run e2e && pnpm run examples",
      "dump:run": "node --enable-source-maps ./packages/va-bds-dumps",
      "e2e": "pnpm run build:src && node --enable-source-maps --import @bedrock-apis/va-core-plugin --import @bedrock-apis/va-loader/node/no-config ./packages/internal-tool/dist/run-compare",
      "e2e:dev": "pnpm build:src && pnpm dump:run && pnpm e2e",
      "example:custom-bds-software": "cd examples/custom-bds-software && pnpm run start",
      "example:custom-bds-software-ts": "cd examples/custom-bds-software-ts && pnpm run start",
      "example:vitest": "cd examples/vitest && pnpm run start",
      "examples": "pnpm run \"/^example:.*/\"",
      "lint": "eslint --cache",
      "repo:init": "pnpm install --frozen-lockfile && pnpm run build:src",
      "repo:publish": "node --enable-source-maps packages/internal-tool/dist/publish",
      "repo:todo": "node --enable-source-maps packages/internal-tool/dist/todo",
      "test": "vitest --run",
      "test:coverage": "vitest --ui --coverage",
      "tlt": "pnpm run typecheck && pnpm run lint && pnpm run test",
      "typecheck": "tsc --pretty --noEmit"
   },
   "devDependencies": {
      "@bedrock-apis/va-bds-dumps": "workspace:^",
      "@bedrock-apis/va-core-plugin": "workspace:^",
      "@bedrock-apis/va-image-generator": "workspace:*",
      "@bedrock-apis/va-images": "workspace:^",
      "@bedrock-apis/va-loader": "workspace:^",
      "@bedrock-apis/va-types": "workspace:^",
      "@eslint/js": "^9.35.0",
      "@minecraft/server": "2.4.0-beta.1.21.120-preview.22",
      "@minecraft/server-net": "1.0.0-beta.1.21.110-preview.25",
      "@minecraft/server-ui": "2.1.0-beta.1.21.120-preview.22",
      "@types/adm-zip": "^0.5.7",
      "@types/node": "^24.5.2",
      "@vitest/coverage-v8": "^3.2.4",
      "@vitest/ui": "3.2.4",
      "eslint": "^9.35.0",
      "oxc-parser": "^0.89.0",
      "prettier": "^3.6.2",
      "prettier-plugin-jsdoc": "^1.3.3",
      "prettier-plugin-pkg": "^0.21.2",
      "rolldown": "1.0.0-beta.38",
      "rolldown-plugin-dts": "^0.16.5",
      "typescript": "^5.9.2",
      "typescript-eslint": "^8.44.0",
      "vitest": "^3.2.4"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/bedrock-apis/virtual-apis.git"
   },
   "homepage": "https://github.com/bedrock-apis/virtual-apis#readme",
   "bugs": {
      "url": "https://github.com/bedrock-apis/virtual-apis/issues"
   },
   "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
   "engines": {
      "node": ">=20.x.x"
   },
   "prettier": {
      "arrowParens": "avoid",
      "endOfLine": "crlf",
      "plugins": [
         "prettier-plugin-jsdoc",
         "prettier-plugin-pkg"
      ],
      "printWidth": 120,
      "quoteProps": "consistent",
      "singleQuote": true,
      "tabWidth": 3,
      "packageSortOrder": [
         "name",
         "description",
         "version",
         "author",
         "license",
         "keywords",
         "private",
         "type",
         "module",
         "workspaces",
         "scripts",
         "dependencies",
         "devDependencies",
         "main",
         "types",
         "exports",
         "files"
      ]
   },
   "pnpm": {
      "overrides": {
         "vite": "npm:rolldown-vite@latest"
      }
   }
}
