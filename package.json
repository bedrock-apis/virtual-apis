   "license": "Apache-2.0",
   "author": "bedrock-apis",
   "type": "module",
   "main": "dist/index.js",
   "scripts": {
      "bundle": "rolldown -c",
      "packagegen": "rolldown -c && node ./dist/build.js",
      "lint": "eslint",
      "test": "vitest",
      "coverage": "vitest --ui --coverage",
      "typecheck": "tsc --noEmit",
      "check": "rolldown -c && node ./dist/build.js && tsc --noEmit && eslint && vitest --run"
   },
   "prettier": {
      "arrowParens": "avoid",
      "endOfLine": "crlf",
      "printWidth": 120,
      "quoteProps": "consistent",
      "singleQuote": true,
      "tabWidth": 3
   },
   "devDependencies": {
      "@types/node": "^22.10.1",
      "@vitest/coverage-istanbul": "2.1.8",
      "@vitest/ui": "2.1.8",
      "eslint": "^9.16.0",
      "prettier": "^3.4.2",
      "rolldown": "1.0.0-beta.1",
      "typescript": "5.7.2",
      "typescript-eslint": "^8.17.0",
      "vitest": "^2.1.8"
   },
   "packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab",
   "engines": {
      "node": ">=20.x.x"
   }
}
