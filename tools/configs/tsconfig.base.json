{
   "compilerOptions": {
      "target": "es2022",
      "lib": ["ES2022"],
      "module": "ESNext",
      "rootDir": "../../",
      "moduleResolution": "bundler",
      "baseUrl": "../../",
      "types": ["../types"],
      "outDir": "../../dist",
      "resolveJsonModule": true,
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "skipLibCheck": true
   }
}
